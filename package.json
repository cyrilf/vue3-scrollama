{
  "name": "vue3-scrollama",
  "type": "module",
  "version": "0.1.3",
  "license": "MIT",
  "private": false,
  "description": "A Vue 3 wrapper for Scrollama",
  "main": "./dist/vue3-scrollama.umd.js",
  "module": "./dist/vue3-scrollama.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-scrollama.js",
      "require": "./dist/vue3-scrollama.umd.js"
    }
  },
  "author": {
    "name": "George (devarteneur) Dimitrakopoulos",
    "email": "hello@georgedimitrakopoulos.com",
    "url": "https://georgedimitrakopoulos.com"
  },
  "keywords": [
    "vue3",
    "scrollytelling",
    "scrollama"
  ],
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devarteneur/vue3-scrollama.git"
  },
  "bugs": {
    "url": "https://github.com/devarteneur/vue3-scrollama/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "scrollama": "^3.2.0"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitepress": "^1.0.0-alpha.34",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-tsc": "^1.0.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
